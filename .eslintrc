{
  "extends": [
    "standard",
    "eslint:recommended",
    "airbnb-base"
  ],
  "plugins": [
    "standard",
    "import",
    "promise"
  ],
  "settings": { "import/resolver": { "node": { "paths": ["./"] } } },
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "modules": true
    }
  }
}
